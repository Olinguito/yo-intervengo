template
  require(from="./yi-button")

  section: content(select="yi-button")

  nav
    yi-button#main(click.delegate="toggle()", title="add new report", icon="plus")
    ul#btns-list
      li(repeat.for="btn of selection")
        yi-button(click.delegate="$parent.navBtnClicked($event)", title="${btn.title}", icon="${btn.icon}")
